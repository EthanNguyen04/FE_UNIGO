{
  "expo": {
    "name": "FE_UNIGO",
    "slug": "FE_UNIGO",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_img.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_img.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.nvt2222.FE_UNIGO"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_img.png"
    },
    "plugins": [
      "expo-dev-client",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_img.png",
          "imageWidth": 200,
          "contentFit": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "edd0acb0-7010-4cba-922c-c669a5a3873d"
      }
    }
  }
}
